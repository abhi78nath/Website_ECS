    var $ = function (id) {
      return document.getElementById(id);
    };
    var inc = 0;
    var out = 0;
    var str = 'ECS MEMBERS  2021-2022';
    var chars = 'ABCDEFGHIJKLMNOPQRSTUVWXYZ123456789@$%&';
    var t;

    var anim = function () {
