        transition:transform 900ms ease
    }
    .box h4{
        font-size: 25px;
        margin-top: 30px;
        color:rgba(255,255,255,0.7)
    }
    .box p{
        line-height:1.9;
        color:rgba(255,255,255,0.7)
    }
    .box a{
        border:none;
        /* background:#fff; */
        padding: 15px 50px;
        display: inline-block;
        margin-top:20px;
        text-decoration:none;
        font-size:14px;
        border-radius:50px;
    }
    .wrapper .box:before{
        content:'';
        background:rgba(255,255,255,0.1);
        width:60%;
        height:100%;
        top:0%;
        left:-125%;
        transform:skew(45deg);
        position:absolute;
        animation-direction:left;
        transition-duration:800ms;
    }
    .wrapper .box:hover{
        transform: translateY(-5%);
    }
    .wrapper .card-content:hover{
        cursor: pointer;
        
    }
    .wrapper .box:hover:before{
        left:150%;
    }
    @media only screen and (max-width:1024px){
